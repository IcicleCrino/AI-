// 最外层是一个js对象，用来存放基础数据
{
    // 对象类型，固定值为“ChatStyleCardBox” 
    "type": "ChatStyleCardBox",
    // 穿搭方案适合的对象，填入男或女
    "gender": "女",
    // 存放数组，数组里的一个对象就是一个穿搭方案
    "styleProjects": [
        {
            // 当前穿搭方案的风格
            "style": "日常",
            // 当前穿搭方案适合的场景，例如约会、办公等
            "background": [
                "咖啡厅",
                "逛街"
            ],
            // 当前穿搭方案的具体穿搭，整体是一个数组，数组里的对象是一件衣物或一件配饰
            "clothes": [
                {
                    // 当前衣物或配饰的类型
                    "type": "上衣",
                    // 当前衣物或配饰的详细描述，在每一个穿搭方案中都要包含衣物描述里的衣物，并且内容都要一致
                    "detail": "一件白色的短袖，其领口为圆形，左侧印有粉红色的“CAMERA PHOTO”字样和“STASHERRY”以及“SEAMLESS”图案，下摆为不规则波浪形，袖子为五分袖，整体风格简约时尚"
                },
                {
                    "type": "下衣",
                    "detail": "条浅蓝色或灰色的牛仔裤，可以选择直筒或微喇叭裤型，使整体穿搭更加休闲舒适"
                },
                {
                    "type": "鞋子",
                    "detail": "双白色帆布鞋"
                },
                {
                    "type": "配饰",
                    "detail": "佩戴一款简约的银色项链，增添时尚感"
                }
            ]
        }
    ]
}